<div class="banner" style="background-image: url(<%= asset_url 'gr-stocks-q8P8YoR6erg-unsplash.jpg' %>);">
    <h1>My Lists</h1>
</div>
<div class="d-flex justify-content-evenly align-items-center mb-4 p-5" id="lists">
  <% @lists.each do |list| %>
    <div class="card" style="width: 18rem;">
      <img class="img-fluid" src=<%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %> class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title">
        <a href="<%= list_path(list.id) %>">
        <%= list.name %>
        </a>
        </h5>
      </div>
    </div>
  <% end %>
</div>
